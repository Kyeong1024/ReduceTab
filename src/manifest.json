{
  "manifest_version": 3,
  "name": "Reduce Tab",
  "description": "reduce tabs",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "ReduceTab Extension"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs", "bookmarks", "alarms"]
}
